{
    "builds": [
        {
            "src": "src/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "./dist/index.htm"
        },
        {
            "src": "/test",
            "dest": "./dist/api/test.route.js"
        },
        {
            "src": "/static",
            "dest": "./dist/static"
        }
    ]
}