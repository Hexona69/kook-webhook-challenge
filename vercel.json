{
    "builds": [
        {
            "src": "src/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/test",
            "dest": "./dist/api/test.route.js"
        }
    ],
    "rewrites": [
        {
            "source": "/static",
            "destination": "/static"
        }
    ]
}