{
    "builds": [
        {
            "src": "src/*.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "./dist/index.htm"
        },
        {
            "source": "/test",
            "destination": "./dist/api/test.route.js"
        },
        {
            "source": "/static",
            "destination": "./dist/static"
        }
    ]
}