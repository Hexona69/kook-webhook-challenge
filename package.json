{
  "name": "kook-webhook-challenge",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run compile && npm run copy",
    "copy": "copyfiles -u 1 \"src/**/*\" -e \"src/**/*.ts\" dist",
    "compile": "tsc",
    "clean": "rimraf dist/*",
    "make": "npm run clean && npm run build && npm run bundle",
    "start": "npm run make && node dist/main.js",
    "bundle": "browserify dist/static/scripts/*.js -o dist/static/scripts/bundle.js -p esmify --extension=.js --extension=.ts"
  },
  "author": "Hexona",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "browser-resolve": "^2.0.0",
    "esmify": "^2.1.1",
    "express": "^4.18.2",
    "get-port": "^6.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17"
  }
}
